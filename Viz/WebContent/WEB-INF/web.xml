<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Viz</display-name>
  <welcome-file-list>
    <welcome-file>/</welcome-file>
    <welcome-file>index</welcome-file>
    <welcome-file>welcome.jsp</welcome-file>
    <welcome-file>welcome.do</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Filter</filter-name>
    <filter-class>org.viz.filter.Main
    </filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Filter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>org.viz.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TreeMap</servlet-name>
    <servlet-class>org.viz.servlet.TreeMapServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TreeMap</servlet-name>
    <url-pattern>/treemap.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>org.viz.servlet.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome.do</url-pattern>
  </servlet-mapping>
</web-app>